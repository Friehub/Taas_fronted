#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/oxisrael/prediction_market/taas-core/node_modules/.pnpm/porto@0.2.35_@tanstack+react-query@5.90.21_react@19.2.4__@types+react@19.2.14_@wagmi+co_27a9ffb2b7974e86167cc6c40b65b551/node_modules/porto/dist/cli/bin/node_modules:/home/oxisrael/prediction_market/taas-core/node_modules/.pnpm/porto@0.2.35_@tanstack+react-query@5.90.21_react@19.2.4__@types+react@19.2.14_@wagmi+co_27a9ffb2b7974e86167cc6c40b65b551/node_modules/porto/dist/cli/node_modules:/home/oxisrael/prediction_market/taas-core/node_modules/.pnpm/porto@0.2.35_@tanstack+react-query@5.90.21_react@19.2.4__@types+react@19.2.14_@wagmi+co_27a9ffb2b7974e86167cc6c40b65b551/node_modules/porto/dist/node_modules:/home/oxisrael/prediction_market/taas-core/node_modules/.pnpm/porto@0.2.35_@tanstack+react-query@5.90.21_react@19.2.4__@types+react@19.2.14_@wagmi+co_27a9ffb2b7974e86167cc6c40b65b551/node_modules/porto/node_modules:/home/oxisrael/prediction_market/taas-core/node_modules/.pnpm/porto@0.2.35_@tanstack+react-query@5.90.21_react@19.2.4__@types+react@19.2.14_@wagmi+co_27a9ffb2b7974e86167cc6c40b65b551/node_modules:/home/oxisrael/prediction_market/taas-core/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/oxisrael/prediction_market/taas-core/node_modules/.pnpm/porto@0.2.35_@tanstack+react-query@5.90.21_react@19.2.4__@types+react@19.2.14_@wagmi+co_27a9ffb2b7974e86167cc6c40b65b551/node_modules/porto/dist/cli/bin/node_modules:/home/oxisrael/prediction_market/taas-core/node_modules/.pnpm/porto@0.2.35_@tanstack+react-query@5.90.21_react@19.2.4__@types+react@19.2.14_@wagmi+co_27a9ffb2b7974e86167cc6c40b65b551/node_modules/porto/dist/cli/node_modules:/home/oxisrael/prediction_market/taas-core/node_modules/.pnpm/porto@0.2.35_@tanstack+react-query@5.90.21_react@19.2.4__@types+react@19.2.14_@wagmi+co_27a9ffb2b7974e86167cc6c40b65b551/node_modules/porto/dist/node_modules:/home/oxisrael/prediction_market/taas-core/node_modules/.pnpm/porto@0.2.35_@tanstack+react-query@5.90.21_react@19.2.4__@types+react@19.2.14_@wagmi+co_27a9ffb2b7974e86167cc6c40b65b551/node_modules/porto/node_modules:/home/oxisrael/prediction_market/taas-core/node_modules/.pnpm/porto@0.2.35_@tanstack+react-query@5.90.21_react@19.2.4__@types+react@19.2.14_@wagmi+co_27a9ffb2b7974e86167cc6c40b65b551/node_modules:/home/oxisrael/prediction_market/taas-core/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/porto@0.2.35_@tanstack+react-query@5.90.21_react@19.2.4__@types+react@19.2.14_@wagmi+co_27a9ffb2b7974e86167cc6c40b65b551/node_modules/porto/dist/cli/bin/index.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/porto@0.2.35_@tanstack+react-query@5.90.21_react@19.2.4__@types+react@19.2.14_@wagmi+co_27a9ffb2b7974e86167cc6c40b65b551/node_modules/porto/dist/cli/bin/index.js" "$@"

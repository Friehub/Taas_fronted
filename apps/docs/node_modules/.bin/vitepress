#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/oxisrael/prediction_market/taas-core/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.48.1_@types+node@25.2.3_@types+react@18.3.28_a_159a32e660a2ef17df9c7c83b1eebfb7/node_modules/vitepress/bin/node_modules:/home/oxisrael/prediction_market/taas-core/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.48.1_@types+node@25.2.3_@types+react@18.3.28_a_159a32e660a2ef17df9c7c83b1eebfb7/node_modules/vitepress/node_modules:/home/oxisrael/prediction_market/taas-core/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.48.1_@types+node@25.2.3_@types+react@18.3.28_a_159a32e660a2ef17df9c7c83b1eebfb7/node_modules:/home/oxisrael/prediction_market/taas-core/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/oxisrael/prediction_market/taas-core/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.48.1_@types+node@25.2.3_@types+react@18.3.28_a_159a32e660a2ef17df9c7c83b1eebfb7/node_modules/vitepress/bin/node_modules:/home/oxisrael/prediction_market/taas-core/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.48.1_@types+node@25.2.3_@types+react@18.3.28_a_159a32e660a2ef17df9c7c83b1eebfb7/node_modules/vitepress/node_modules:/home/oxisrael/prediction_market/taas-core/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.48.1_@types+node@25.2.3_@types+react@18.3.28_a_159a32e660a2ef17df9c7c83b1eebfb7/node_modules:/home/oxisrael/prediction_market/taas-core/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
